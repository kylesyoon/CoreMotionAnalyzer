PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-CoreMotionAnalyzer
PODS_ROOT = ${SRCROOT}/Pods